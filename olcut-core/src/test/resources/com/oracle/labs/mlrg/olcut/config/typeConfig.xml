<!--
    Description:
        A configuration file for testing string configurations.
-->

<config>
    
    <component name="default" type="com.oracle.labs.mlrg.olcut.config.BasicConfigurable">
    </component>

    <component name="type-a" type="com.oracle.labs.mlrg.olcut.config.BasicConfigurable">
        <property name="s" value="one"/>
        <property name="i" value="2"/>
        <property name="d" value="3.0"/>
    </component>

    <component name="type-b" type="com.oracle.labs.mlrg.olcut.config.BasicConfigurable">
        <property name="s" value="two"/>
        <property name="i" value="3"/>
        <property name="d" value="6.3"/>
    </component>
    
    <component name="type-list" type="com.oracle.labs.mlrg.olcut.config.ListTypeConfigurable">
        <propertylist name="list">
            <item>default</item>
            <item>type-a</item>
            <item>type-b</item>
        </propertylist>
    </component>
    
</config>