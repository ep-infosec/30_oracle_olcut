<!--
    Document   : componentListConfig.xml
    Created on : December 11, 2007, 7:58 AM
    Author     : stgreen
    Description:
        A configuration file to test component lists.
-->

<config>
    <component name="simple" type="com.oracle.labs.mlrg.olcut.config.ListConfigurable">
        <propertylist name="list">
            <item>simp</item>
            <item>strings</item>
        </propertylist>
    </component>
    
    <component name="bad" type="com.oracle.labs.mlrg.olcut.config.ListConfigurable">
        <propertylist name="list">
            <item>non_existent</item>
            <item>strings</item>
        </propertylist>
    </component>
    
    <component name="typed" type="com.oracle.labs.mlrg.olcut.config.ListConfigurable">
        <propertylist name="list">
            <type>com.oracle.labs.mlrg.olcut.config.StringConfigurable</type>
        </propertylist>
    </component>
    
    <component name="dualtyped" type="com.oracle.labs.mlrg.olcut.config.ListConfigurable">
        <propertylist name="list">
            <type>com.oracle.labs.mlrg.olcut.config.StringConfigurable</type>
            <type>com.oracle.labs.mlrg.olcut.config.SimpleConfigurable</type>
        </propertylist>
    </component>

    <component name="combotyped" type="com.oracle.labs.mlrg.olcut.config.ListConfigurable">
        <propertylist name="list">
            <item>simp</item>
            <type>com.oracle.labs.mlrg.olcut.config.StringConfigurable</type>
        </propertylist>
    </component>

    <component name="stringconfigurablearray" type="com.oracle.labs.mlrg.olcut.config.ArrayStringConfigurable">
        <propertylist name="stringArray">
            <item>strings</item>
            <item>strings2</item>
            <item>strings3</item>
        </propertylist>
    </component>

    <component name="stringconfigurablearraybroken" type="com.oracle.labs.mlrg.olcut.config.ArrayStringConfigurable">
        <propertylist name="stringArray">
            <item>strings</item>
            <item>monkeys</item>
            <item>gibbons</item>
        </propertylist>
    </component>
    
    <component name="simp" type="com.oracle.labs.mlrg.olcut.config.SimpleConfigurable">
        <property name="simple" value="10"/>
    </component>
    
    <component name="simp2" type="com.oracle.labs.mlrg.olcut.config.SimpleConfigurable">
        <property name="simple" value="20"/>
    </component>
    
    <component name="strings" type="com.oracle.labs.mlrg.olcut.config.StringConfigurable">
        <property name="one" value="alpha"/>
        <property name="two" value="beta"/>
        <property name="three" value="charlie"/>
    </component>

    <component name="strings2" type="com.oracle.labs.mlrg.olcut.config.StringConfigurable">
        <property name="one" value="one"/>
        <property name="two" value="two"/>
        <property name="three" value="three"/>
    </component>

    <component name="strings3" type="com.oracle.labs.mlrg.olcut.config.StringConfigurable">
        <property name="one" value="un"/>
        <property name="two" value="deux"/>
        <property name="three" value="trois"/>
    </component>
</config>
